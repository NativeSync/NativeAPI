{
  "type": "nativeapi",
  "base_url": "https://test.nativeapi.io/"
  "version": "v1",
  "credentials_template": {
    "account_id": "Account ID",
    "secret_key": "Secret Key"
  },
  "auth_type": "get_params",
  "date_format": "YY-mm-dd HH:ii:ss",
  "sync_objects": {
    "lead": {
      "fields": {
        "firstName": {
          "label": "First Name",
          "dataType": "string"
        },
        "lastName": {
          "label": "Last Name",
          "dataType": "string"
        },
        "emailAddress": {
          "label": "Email Address",
          "dataType": "string"
        },
        "companyName": {
          "label": "Company Name",
          "dataType": "string"
        }
      },
      "unique_keys": [
        [
          "emailAddress"
        ],
        [
          "firstName",
          "lastName",
          "companyName"
        ]
      ],
      "required_fields": [
        "emailAddress"
      ],
      "client_id": "companyProfileID",
      "primary_key": "id",
      "update_timestamp": "updateTimestamp"
    }
  }
}
